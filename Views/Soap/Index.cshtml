@model IEnumerable<SoapMvcApp.Models.Soap>

<h1>Soap Products</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Scent</th>
            <th>Price</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var soap in Model)
        {
            <tr>
                <td>@soap.Id</td>
                <td>@soap.Name</td>
                <td>@soap.Scent</td>
                <td>@soap.Price</td>
                <td>@soap.Description</td>
            </tr>
        }
    </tbody>
</table>
